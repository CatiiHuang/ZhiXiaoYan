
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 13vw;
    border-top: 1rpx solid #d7d7d7; 
}
.tabbar_nav:first-of-type{
   margin-right: 32rpx;
}
.tabbar_nav:last-of-type{
   margin-left: 32rpx;
}
.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
    height: 100%;
    letter-spacing: 4rpx;
}

.tabbar_icon{
    width: 6.5vw;
    height:6.5vw;
    margin-bottom: 0;
}

